// Nhập số n và in ra các ước của số n đó
#include <stdio.h>

int main() {
    int n;
    printf("Nhap so n: ");
    scanf("%d", &n);

    printf("Cac uoc cua %d là: ", n);
    int i;
	for ( i = 1; i <= n; i++) {
        if (n % i == 0) {
            printf("%d ", i);
        }
    }
    printf("\n");
    return 0;
}
