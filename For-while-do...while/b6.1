// Tìm UCLN, BCNN của 2 số nguyên được nhập từ bàn phím
#include <stdio.h>

int gcd(int a, int b) {
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}

int main() {
    int a, b;
    printf("Nhap hai so: ");
    scanf("%d %d", &a, &b);

    int ucln = gcd(a, b);
    int bcnn = (a * b) / ucln;

    printf("UCLN: %d\n", ucln);
    printf("BCNN: %d\n", bcnn);

    return 0;
}
