#include <stdio.h>

int main() {
    int arr[10];
    
    printf("Nhap 10 so nguyen:\n");
    int i;
    for (i = 0; i < 10; i++) {
        printf("Phan tu [%d]: ", i);
        scanf("%d", &arr[i]);
    }
    
    printf("Mang Ä‘a nhap: ");
    for (i = 0; i < 10; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");
    
    return 0;
}
